{
  "version": "2.0.0-alpha-*",

  "copyright": "2013-2016 Patrick Lioi",

  "packOptions": {
    "summary": "`dotnet test` integration for the Fixie test framework.",
    "owners": [ "Patrick Lioi" ],
    "iconUrl": "https://raw.github.com/fixie/fixie/master/img/fixie_256.png",
    "projectUrl": "https://fixie.github.io",
    "licenseUrl": "https://github.com/fixie/fixie/blob/master/LICENSE.txt",
    "requireLicenseAcceptance": false,
    "repository": {
